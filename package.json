{
  "name": "mongo-stash",
  "version": "0.1.0",
  "description": "Caching wrapper for MongoDB collections.",
  "main": "lib/index.js",
  "scripts": {
    "test": "gulp coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jluchiji/mongo-stash.git"
  },
  "keywords": [
    "mongodb",
    "cache"
  ],
  "author": "Denis Luchkin-Zhou <denis@ricepo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jluchiji/mongo-stash/issues"
  },
  "homepage": "https://github.com/jluchiji/mongo-stash#readme",
  "eslintConfig": {
    "root": true,
    "extends": "airbnb/base",
    "rules": {
      "new-cap": 0,
      "no-undef": 2,
      "id-length": 0,
      "func-names": 0,
      "no-console": 1,
      "key-spacing": 0,
      "padded-blocks": 0,
      "no-multi-spaces": 0,
      "no-const-assign": 2,
      "no-param-reassign": 0,
      "comma-dangle": [
        1,
        "never"
      ]
    },
    "ecmaFeatures": {
      "generators": true
    },
    "envs": {
      "node": true,
      "commonjs": true,
      "browser": false
    }
  },
  "babel": {
    "auxiliaryCommentBefore": "istanbul ignore next",
    "optional": [
      "bluebirdCoroutines"
    ]
  },
  "dependencies": {
    "bluebird": "^2.10.2",
    "bson-objectid": "^1.1.1",
    "lodash": "^3.10.1",
    "lru-cache": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^1.7.1",
    "babel-eslint": "^4.1.3",
    "del": "^2.0.2",
    "eslint-config-airbnb": "^0.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-changed": "^1.3.0",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul": "^0.10.1",
    "gulp-mocha": "^2.1.3",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0"
  }
}
